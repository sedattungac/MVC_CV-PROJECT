@model Mvc.Models.Entity.TblSosyalMedya
@{
    ViewBag.Title = "SosyalMedyaGetir";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}



<br>
<h2>Sosyal Medya Hesap Düzenleme Sayfası</h2>
<br />
<form method="post">

    @Html.Label("Sosyal Medya*")
    @Html.TextBoxFor(x => x.Ad, new { @class = "form-control" })
    @*@Html.ValidationMessageFor(x => x.Yetenek, "", new { @style = "color:red" })*@
    <br />
    @Html.Label("Link*")
    @Html.TextBoxFor(x => x.Link, new { @class = "form-control" })
    @*@Html.ValidationMessageFor(x => x.Oran, "", new { @style = "color:red" })*@
    <br />
    @Html.Label("İkon*")
    @Html.TextBoxFor(x => x.ikon, new { @class = "form-control" })
    @*@Html.ValidationMessageFor(x => x.Oran, "", new { @style = "color:red" })*@
    <br />
    <button class="btn btn-warning">Güncelle</button>
</form>

